          <h3 class="widget-title">
           <!-- <%= site.data.imagesj.length %> :Popular Images themes\corporate\layout\_partial\gallerygtz.ejs </h3>
         -->
          <div class="widget">
            <div class="row">
              <div class="col-md-12">
                <% for (item in site.data.imagesj) { %>
                  <img class="wow fadeIn img-responsive" src="<%= site.data.imagesj[item].path %>">
                  <h4>
                    item
                    <%= item%> |
                      <%= site.data.imagesj[item].id %>-
                        <%= site.data.imagesj[item].location %>
                  </h4>
                  </img>
                  <% } %>
              </div>
            </div>
          </div>
        </div>
        <% } %>



